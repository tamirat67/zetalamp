<section class="colorlib-contact" data-section="followup">
    <div class="colorlib-narrow-content">
        <div class="row">
            <div class="col-md-12 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
                <span class="heading-meta">Item request from store and purchase order request status</span>
                <h2 class="colorlib-heading animate-box">Request Status Followup</h2>
            </div>
        </div>

        <div class="row" id="UserRequestFollowup">
        </div>
    </div>
</section>


<script>
    $(document).ready(function () {
        $.ajax({
            url: "#",
            success: function (result) {
                $("#UserRequestFollowup").html(result);
            }, error: function (xhr, status, error) {
                $("#UserRequestFollowup").html("Problem" + error);

            }
        });
    });
</script>
