<section class="colorlib-services" data-section="stock">
    <div class="colorlib-narrow-content">
        <div class="row">
            <div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
                <span class="heading-meta">What item do you need?</span>
                <h2 class="colorlib-heading">Stores</h2>
            </div>
        </div>
        <div class="row row-pt-md" id="storesList">
        </div>
    </div>
</section>

<script>
    $(document).ready(function () {
        $.ajax({
            url: "#",
            success: function (result) {
                $("#storesList").html(result);
            }, error: function (xhr, status, error) {
                $("#storesList").html("Problem" + error);

            }
        });
    });
</script>
