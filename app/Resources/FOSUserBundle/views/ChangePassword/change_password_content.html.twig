{% trans_default_domain 'FOSUserBundle' %}


   <div class="row">
        <div class="col-md-12 well well-sm">
   
{% block name %}
  <div class="text-info center">
       <h2>Use strong and compiex password</h2>
   </div>
<hr />
{% endblock name %}

           
 {{ form_start(form, { 'action': path('fos_user_change_password'),
  'attr': { 'class': 'fos_user_change_password' } }) }}
    {{ form_widget(form) }} 
  <div >
        <input type="submit" class="btn btn-primary btn-sm"  value="{{ 'change_password.submit'|trans }}" />
    </div>

</div>
</div>
{{ form_end(form) }}
